#include <stdio.h>

int main() 
{
    int n;
   printf("enter the number:");
   scanf("%d",&n);
   for(int i=0;i<=n;i++)
   {
       if((n-i)%2==0)
       {
           int arr[100];
           int k=0;
           for(int j=0;j<i;j++)
           {
               arr[k]=1;
               k++;
           }
           for(int j=0;j<(n-i)/2;j++)
           {
               arr[k]=2;
               k++;
           }
           for(int m=0;m<k;m++)
           {
               printf("%d",arr[m]);
           }
           printf("\n");
           
       }
   }
}
